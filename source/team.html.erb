

<div class="container text-center padded features">
<h1>Our Team</h1>
<ul class="list-inline text-center">
  <% ["anne", "chris", "alf"].each do |member| %>
    <li>
      <h3><%= member.capitalize %></h3>
      <%= image_tag "#{member}.jpg", class: "img-circle avatar-large" %>
    </li>
  <% end %>
</ul>
</div>
<div class="container text-center padded features">
  <% link_to "mailto:contact@airbnb.com" do %>
  <i class="fa fa-envelope" aria-hidden="true"></i> contact@airbnb.com
  <% end %>
</div>
